<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ocbc.les.modules.system.mapper.UserInfoMapper">

    <resultMap id="BaseResultMap" type="com.ocbc.les.modules.system.entity.UserInfo">
            <id property="id" column="id" />
            <result property="userId" column="user_id" />
            <result property="password" column="password" />
            <result property="userNameZh" column="user_name_zh" />
            <result property="userNameEn" column="user_name_en" />
            <result property="orgId" column="org_id" />
            <result property="status" column="status" />
            <result property="lastLoginTime" column="last_login_time" />
            <result property="lastLoginIp" column="last_login_ip" />
            <result property="delFlag" column="del_flag" />
            <result property="createBy" column="create_by" />
            <result property="createOrgId" column="create_org_id" />
            <result property="createTime" column="create_time" />
            <result property="updateBy" column="update_by" />
            <result property="updateOrgId" column="update_org_id" />
            <result property="updateTime" column="update_time" />
            <result property="remark" column="remark" />
    </resultMap>

    <sql id="Base_Column_List">
        id,user_id,password,user_name_zh,user_name_en,org_id,
        status,last_login_time,last_login_ip,del_flag,create_by,
        create_org_id,create_time,update_by,update_org_id,update_time,
        remark
    </sql>

    <insert id="addUser">
        INSERT INTO user_info (
            user_id, password, user_name_zh, user_name_en, org_id,
            status, last_login_time, last_login_ip, del_flag,
            create_by, create_org_id, create_time,
            update_by, update_org_id, update_time, remark
        ) VALUES (
            #{userId}, #{password}, #{userNameZh}, #{userNameEn}, #{orgId},
            #{status}, #{lastLoginTime}, #{lastLoginIp}, '0',
            #{createBy}, #{createOrgId}, #{createTime},
            #{updateBy}, #{updateOrgId}, #{updateTime}, #{remark}
        )
    </insert>
</mapper>
